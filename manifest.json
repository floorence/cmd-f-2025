{
    "manifest_version": 3,
    "name": "HealthShield",
    "version": "1.0",
    "description": "AI-powered chrome extension that detects health misinformation on Twitter in real-time.",
  
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://x.com/*"
        ]
      }
    ]
  }